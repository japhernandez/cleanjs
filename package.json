{
  "name": "@cleanjs/framework",
  "version": "1.0.0",
  "description": "Plugin for API Rest Full development, based on Clean Architecture, IoC and Dependency Injection.",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsc -w -p tsconfig.json",
    "watch": "nodemon --exec \"npm run build && npm run start\" --watch src --ext ts",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/john-piedrahita/cleanjs.git"
  },
  "keywords": [
    "Framework",
    "Clean",
    "NodeJs",
    "Express",
    "Clean",
    "Architecture"
  ],
  "author": "citymanager02@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/john-piedrahita/cleanjs/issues"
  },
  "homepage": "https://github.com/john-piedrahita/cleanjs#readme",
  "_moduleAliases": {
    "@": "dist"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fast-safe-stringify": "^2.0.8",
    "iterare": "^1.2.1",
    "module-alias": "^2.2.2",
    "object-hash": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
