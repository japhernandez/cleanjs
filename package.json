    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.29.0",
    "eslint-plugin-import": "^2.23.4",
    "nodemon": "^2.0.9",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ]
